<h1>users#index</h1>

<%= render 'users/info', user: @user %>

<%= render 'books/new', book: @book %>

<div class="col-md-8">
       <h1>Users</h1>
       <table>
        <thead>
         <tr>
          <th>image</th>
          <th>name</th>
          <th colspan="3"></th>
         </tr>
        </thead>

        <tbody>
         <% @users.each do |user| %>
         <tr>
          <td><!--%= user.image %--></td>
          <td><%= user.name %></td>
          <td><!--%= link_to 'Show', user(@user.id) --></td>
         </tr>
         <% end %>
        </tbody>
       </table>
      </div>

  </div>
</div>